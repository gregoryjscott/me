{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["lint", "prep", "build"]
      }
    }
  },
  "targetDefaults": {
    "lint": {
      "dependsOn": ["^lint"]
    },
    "prep": {
      "dependsOn": ["^prep"]
    },
    "build": {
      "dependsOn": ["^build"]
    }
  }
}
