{
  "name": "www",
  "version": "1.0.0",
  "description": "Manages gregoryjscott.com",
  "scripts": {
    "bootstrap": "gem install bundler jekyll && bundle update",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "build": "bundle exec jekyll build --verbose",
    "prep": "ts-node _scripts/prep.ts",
    "gpt": "ts-node _scripts/gpt.ts",
    "www": "bundle exec jekyll serve",
    "open": "open http://127.0.0.1:4000",
    "deploy": "gh-pages --dist _site --src '**' --repo https://github.com/gregoryjscott/gregoryjscott.github.io.git --branch master --message 'Generated by me'"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.11",
    "eslint": "^8.29.0",
    "eslint-config-custom": "*",
    "gh-pages": "^3.2.3",
    "gpt-3-encoder": "^1.1.4",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "openai": "^3.1.0",
    "prettier": "2.8.0",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "typescript": "^4.9.3",
    "yaml": "^2.1.3"
  }
}
