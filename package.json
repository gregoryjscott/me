{
  "repository": {
    "type": "git",
    "url": "https://github.com/gregoryjscott/me.git"
  },
  "scripts": {
    "bootstrap": "gem install bundler jekyll && bundle update && gem specific_install http://github.com/gregoryjscott/jekyll-load.git other-data",
    "prep": "ts-node prep.ts",
    "build": "bundle exec jekyll build",
    "serve": "bundle exec jekyll serve",
    "open": "open http://127.0.0.1:4000",
    "deploy": "gh-pages --dist _site --src '**' --repo https://github.com/gregoryjscott/gregoryjscott.github.io.git --branch master --message 'Generated by me'"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  }
}
